<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Replayer.WinForms.Ui.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
      <section name="Replayer.WinForms.Ui.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <userSettings>
    <Replayer.WinForms.Ui.Properties.Settings>
      <setting name="UiFont" serializeAs="String">
        <value>Arial, 18pt</value>
      </setting>
      <setting name="LastLoadedCompilationPath" serializeAs="String">
        <value />
      </setting>
    </Replayer.WinForms.Ui.Properties.Settings>
  </userSettings>

  <log4net>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="${LOCALAPPDATA}\Replayer\Replayer-Log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value=".yyyy-MM-dd'.txt'" />
      <maxSizeRollBackups value="30" />
      <staticLogFileName value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%d{dd.MM.yyyy HH:mm:ss,fff} %-5level [%thread]: %message%newline" />
      </layout>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <!--Logging to the Visual Studio trace output-->
    <appender name="TraceAppender" type="log4net.Appender.TraceAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%d{dd.MM.yyyy HH:mm:ss,fff} %-5level [%thread]: %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="RollingFileAppender" />
      <!--Logging to the Visual Studio trace output-->
      <appender-ref ref="TraceAppender"/>
    </root>
  </log4net>

  <runtime>
